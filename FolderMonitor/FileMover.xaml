<Window x:Class="FolderMonitor.FileMover"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="FileMover" Height="30" Width="260"
    ResizeMode="NoResize"
    ShowInTaskbar="False" WindowStyle="None"
    MouseDown="HideFileMover" Cursor="Hand"
    ToolTip="Click to hide..." Topmost="True"
    BorderThickness="1" BorderBrush="#FF6E6E6E"
    Foreground="White" Background="#FF494949" >
    <StackPanel Margin="2,2,0,2" Name="StackContent" HorizontalAlignment="Center">
        
    </StackPanel>
</Window>
